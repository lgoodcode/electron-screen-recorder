{
  "name": "electron-react-typescript-webpack",
  "version": "0.0.1",
  "description": "Fast, secure, unopinionated, Electron boilerplate",
  "author": "Lawrence Good",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development ts-node scripts/start.ts",
    "start:main": "cross-env NODE_ENV=development electronmon -r ts-node/register ./src/main/main.ts",
    "start:preload": "cross-env NODE_ENV=development webpack --config config/webpack.config.preload.ts",
    "build": "ts-node scripts/clean.ts && ts-node scripts/build.ts --run",
    "build:main": "cross-env NODE_ENV=production webpack --config config/webpack.config.main.ts",
    "build:renderer": "cross-env NODE_ENV=production webpack --config config/webpack.config.renderer.ts",
    "test": "jest",
    "test:watch": "jest --watchAll"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "<rootDir>/scripts/checkBuild.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/__tests__/setupTests.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost/"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.erb/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    }
  },
  "electronmon": {
    "patterns": [
			"!**/*.*",
      "src/main/*"
    ],
    "logLevel": "quiet"
  },
  "dependencies": {
    "bulma": "^0.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^28.1.3",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "chalk": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "electron": "^19.0.6",
    "electron-builder": "^23.1.0",
    "electron-debug": "^3.2.0",
    "electron-devtools-installer": "^3.2.0",
    "electron-log": "^4.4.8",
    "electron-updater": "^5.0.5",
    "electronmon": "^2.0.2",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "fork-ts-checker-webpack-plugin": "^7.2.11",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "prettier": "^2.7.1",
    "react-refresh": "^0.14.0",
    "rimraf": "^3.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.3",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
